{
	"name":"World Server",
	"version":1,
	"description":"All-in-one 'world' incl. mongodb, rabbitmq, memcached, nginx, and more.",
	"env" : "Disposable",
	"isProd" : false,
	"isContainer" : true,
	"facilities":{
		"http":{
			"port":HTTP_PORT
		},
		"storage":{
			"mongoDB":{
				"uris":["HOSTIP:MONGO_PORT"]
			}
		},
		"queue":{
			"rabbitMQ":{
				"hosts":[{
					"host":"HOSTIP",
					"port":RABBIT_PORT
					}],
				"uid":"guest",
				"pwd":"guest"
			}
		},
		"cache":{
			"memcached":{
				"uris":["HOSTIP:MEMCACHE_PORT"]
			}
		}
	},
	"applications":[
	]
}
